package com.web.dao;

/**
 * 购物车的数据访问层接口
 * @author 王佳祺
 *
 */
public interface CartDao {
	/**
	 * 添加购物车
	 * @param id
	 * @param sweet
	 * @param temp
	 * @param spec
	 * @param num
	 * @return
	 */
	 public boolean addCart(int UserID, int DrinkID, String sweet, String temp, String spec, int num);
}
