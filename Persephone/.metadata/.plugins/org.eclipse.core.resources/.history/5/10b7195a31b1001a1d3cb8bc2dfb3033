			String sql = "INSERT INTO `user`(UserName,UserPwd,UserPhone)"
					+ "VALUES (?,?,?)";
			
			// 编译sql
			PreparedStatement ps = conn.prepareStatement(sql);
			
			// 设置参数
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		return false;
	}

}
