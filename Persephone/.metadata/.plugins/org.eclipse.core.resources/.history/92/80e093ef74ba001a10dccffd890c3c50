package com.web.dao;

import java.util.List;

import com.web.entity.Admin;

public interface AdminDao {
	/**
	 * 管理员登陆
	 * @param AdminName
	 * @param AdminPwd
	 * @return
	 */
	public Admin login(String AdminName, String AdminPwd);
	
	/**
	 * 后台显示管理员信息
	 * @return
	 */
	public List<Admin> showAdmin();
}
